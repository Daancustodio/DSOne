{
  "_version": "1.0.1",
  "sap.app": {
    "_version": "1.0.0",
    "id": "DecisionsOne",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "title": "DecisionsOne",
    "description": "Addon para aprovações de documentos",
    "applicationVersion": {
      "version": "1.0.0"
    }
  },
  "sap.ui": {
    "_version": "1.3.0",
    "technology": "UI5",
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    },
    "supportedThemes": [
      {
        "Id": "sap_belize",
        "Description": "Belize"
      },
      {
        "Id": "sap_belize_plus",
        "Description": "Belize Plus"
      },
      {
        "Id": "sap_bluecrystal",
        "Description": "Blue Crystal"
      },
      {
        "Id": "sap_belize_hcb",
        "Description": "Alto Constraste Preto"
      },
      {
        "Id": "sap_belize_hcw",
        "Description": "Alto Contraste Branco"
      }
    ]
  },
  "sap.ui5": {
    "_version": "1.2.0",
    "rootView": "DecisionsOne.src.app.App",
    "config": {
      "serviceUrl": "https://hanab1:4300/oDataService/services",
      "b1s": "http://hanab1:50001/b1s/v1"
    },
    "dependencies": {
      "minUI5Version": "1.34.0",
      "libs": {
        "sap.ui.core": {
          "minVersion": "1.34.0"
        },
        "sap.m": {
          "minVersion": "1.34.0"
        },
        "sap.ui.layout": {
          "minVersion": "1.34.0"
        }
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "DecisionsOne.i18n.i18n"
        }
      }
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "controlId": "app",
        "controlAggregation": "pages",
        "bypassed": {
          "target": "notFound"
        }
      },
      "routes": [
        {
          "pattern": "app",
          "name": "app",
          "target": "app"
        },
        {
          "pattern": "",
          "name": "login",
          "target": "login"
        },
        {
          "pattern": "dashBoard",
          "name": "dashBoard",
          "target": "dashBoard"
        },
        {
          "pattern": "settings/:userName:",
          "name": "settings",
          "target": "settings"
        },
        {
          "pattern": "user",
          "name": "user",
          "target": "user"
        },
        {
          "pattern": "drafts",
          "name": "drafts",
          "target": "drafts"
        },
        {
          "pattern": "drafts/{docEntry}/{code}",
          "name": "draftsDetail",
          "target": "draftsDetail"
        },
        {
          "pattern": "purchaseRequest",
          "name": "purchaseRequest",
          "target": "purchaseRequest"
        },
        {
          "pattern": "draftsEdit/{docEntry}",
          "name": "draftsEdit",
          "target": "draftsEdit"
        },
        {
          "pattern": "userDefinedFieldsList",
          "name": "userDefinedFieldsList",
          "target": "userDefinedFieldsList"
        },
        {
          "pattern": "userDefinedFieldsDetail/{TableName}/{FieldID}",
          "name": "userDefinedFieldsDetail",
          "target": "userDefinedFieldsDetail"
        },
        {
          "pattern": "userDefinedTables",
          "name": "userDefinedTables",
          "target": "userDefinedTables"
        },
        {
          "pattern": "userDefinedTablesDetail/{TableName}",
          "name": "userDefinedTablesDetail",
          "target": "userDefinedTablesDetail"
        },
        {
          "pattern": "requestPurchaseDetail/{docEntry}",
          "name": "requestPurchaseDetail",
          "target": "requestPurchaseDetail"
        },
        {
          "pattern": "requestPurchaseEdit/{docEntry}",
          "name": "requestPurchaseEdit",
          "target": "requestPurchaseEdit"
        },
        {
          "pattern": "requestPurchaseCreate",
          "name": "requestPurchaseCreate",
          "target": "requestPurchaseCreate"
        }
      ],
      "targets": {
        "dashBoard": {
          "viewName": "DashBoard",
          "viewLevel": 2,
          "transition": "flip",
          "viewPath": "DecisionsOne.src.pages.dashboard"
        },
        "settings": {
          "viewName": "Edit",
          "viewLevel": 3,
          "viewPath": "DecisionsOne.src.pages.settings",
          "transition": "flip"
        },
        "login": {
          "viewName": "Login",
          "viewLevel": 3,
          "viewPath": "DecisionsOne.src.pages.security",
          "transition": "flip"
        },
        "exemplos": {
          "viewName": "CustomControls",
          "viewLevel": 3,
          "viewPath": "DecisionsOne.src.pages.exemplos",
          "transition": "flip"
        },
        "app": {
          "viewName": "App",
          "viewLevel": 3,
          "viewPath": "DecisionsOne.src.app"
        },
        "user": {
          "viewName": "User",
          "viewLevel": 3,
          "viewPath": "DecisionsOne.src.pages.user"
        },
        "drafts": {
          "viewName": "List",
          "viewLevel": 3,
          "viewPath": "DecisionsOne.src.pages.drafts"
        },
        "draftsDetail": {
          "viewName": "Detail",
          "viewLevel": 3,
          "viewPath": "DecisionsOne.src.pages.drafts"
        },
        "draftsEdit": {
          "viewName": "Edit",
          "viewLevel": 3,
          "viewPath": "DecisionsOne.src.pages.drafts"
        },
        "userDefinedFieldsList": {
          "viewName": "UserDefinedFieldsList",
          "viewLevel": 3,
          "viewPath": "DecisionsOne.src.pages.userDefinedFieldsList"
        },
        "userDefinedFieldsDetail": {
          "viewName": "UserDefinedFieldsDetail",
          "viewLevel": 3,
          "viewPath": "DecisionsOne.src.pages.userDefinedFieldsDetail"
        },
        "userDefinedTables": {
          "viewName": "UserDefinedTables",
          "viewLevel": 3,
          "viewPath": "DecisionsOne.src.pages.userDefinedTables"
        },
        "userDefinedTablesDetail": {
          "viewName": "UserDefinedTablesDetail",
          "viewLevel": 3,
          "viewPath": "DecisionsOne.src.pages.userDefinedTablesDetail"
        },
        "purchaseRequest": {
          "viewName": "List",
          "viewLevel": 3,
          "viewPath": "DecisionsOne.src.pages.purchaseRequest"
        },
        "requestPurchaseDetail": {
          "viewName": "RequestPurchaseDetail",
          "viewLevel": 3,
          "viewPath": "DecisionsOne.src.pages.requestPurchaseDetail"
        },
        "requestPurchaseEdit": {
          "viewName": "RequestPurchaseEdit",
          "viewLevel": 3,
          "viewPath": "DecisionsOne.src.pages.requestPurchaseEdit"
        },
        "requestPurchaseCreate": {
          "viewName": "RequestPurchaseCreate",
          "viewLevel": 3,
          "viewPath": "DecisionsOne.src.pages.requestPurchaseCreate"
        }
      }
    }
  },
  "name": "DecisionsOne",
  "short_name": "DOne",
  "start_url": "index.html",
  "display": "standalone",
  "orientation": "portrait",
  "icons": [
    {
      "src": "img/icons/icon-72x72.png",
      "sizes": "72x72",
      "type": "image/png"
    },
    {
      "src": "img/icons/icon-96x96.png",
      "sizes": "96x96",
      "type": "image/png"
    },
    {
      "src": "img/icons/icon-128x128.png",
      "sizes": "128x128",
      "type": "image/png"
    },
    {
      "src": "img/icons/icon-144x144.png",
      "sizes": "144x144",
      "type": "image/png"
    },
    {
      "src": "img/icons/icon-152x152.png",
      "sizes": "152x152",
      "type": "image/png"
    },
    {
      "src": "img/icons/icon-192x192.png",
      "sizes": "192x192",
      "type": "image/png"
    },
    {
      "src": "img/icons/icon-384x384.png",
      "sizes": "384x384",
      "type": "image/png"
    },
    {
      "src": "img/icons/icon-512x512.png",
      "sizes": "512x512",
      "type": "image/png"
    }
  ],
  "background_color": "#576574",
  "theme_color": "#4e8ef7"
}